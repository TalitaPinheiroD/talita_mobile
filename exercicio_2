rt( "Programa para saber se passou de ano" );

var primeiranota= Number( prompt ("Digite a nota do primeiro trimestre") );
console.log (primeiranota)

var segundanota= Number (prompt ("digite a nota do segundo trimestre") );
console.log (segundanota)
  
var terceiranota= Number (prompt("digite a nota do terceiro trimestre") );
console.log (terceiranota)

console.log (primeiranota + segundanota + terceiranota)/3
